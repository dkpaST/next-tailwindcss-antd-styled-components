{
  "name": "doggie-commerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "postcss src/styles/css/index.css -o src/styles/css/tailwind.css",
    "watch:css": "postcss src/styles/css/index.css -o src/styles/css/tailwind.css",
    "build-js": "next build",
    "build": "npm-run-all build:css build-js",
    "dev:watch": "npm-run-all -p watch:css dev ",
    "dev": "next dev",
    "start": "next start",
    "std": "semistandard && next dev",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "telemetry:disable": "next telemetry disable",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@chakra-ui/react": "^1.0.0",
    "@emotion/core": "^10.0.28",
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@theme-ui/presets": "^0.3.0",
    "@zeit/next-css": "^1.0.1",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-macros": "^2.8.0",
    "emotion-theming": "^10.0.27",
    "framer-motion": "^2.9.4",
    "less": "^3.11.1",
    "less-vars-to-js": "^1.3.0",
    "next": "10.0.1",
    "next-images": "^1.6.2",
    "node-sass": "^4.14.1",
    "null-loader": "^3.0.0",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "twin.macro": "^1.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/core": "^11.0.0",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^9.5.0",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "babel-jest": "^25.2.6",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "babel-plugin-styled-components": "^1.11.1",
    "commitlint-config-gitmoji": "^1.0.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.7",
    "next-compose-plugins": "^2.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.13.1",
    "semistandard": "*",
    "tailwindcss": "^1.9.6",
    "typescript": "^4.0.5"
  }
}
